    document.addEventListener('mousemove', parallax )
    function parallax(e) {
      document.querySelectorAll('.layer').forEach(move => {
        let movingValue = move.getAttribute("data-speed")
        let x = (e.clientX * movingValue)/400
        let y = (e.clientY * movingValue)/400
        move.style.transform = "translateX("+x+"px) translateY("+y+"px)"

      })
    }

    let searchLabText = document.getElementById('laboratorijeRadioButton')
