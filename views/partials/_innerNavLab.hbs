<header id="smallHeaderLab">
  <nav class="navbar navbar-expand-md navbar-dark mb-auto z" >
    <div class="container">
      <a href="/" class="navbar-brand">
        <img src="/images/logoSmallSlice.svg" alt="labcube"></a>
        <a href="/logout" class="nav-link link-unstyled dashboardTabs" id="loagout-tab"> <i class="fa fa-sign-out-alt text-white" title="odjava" alt="odjava"></i></a>
      </div><!-- collapse -->
    </div><!-- container end -->
  </nav><!-- nav end -->
  <div class="blueOverlayLab">
    <div class="container ">

      <div class="labDashDetails" style="position:relative;">
        <div class="feedbackLabDash" style="">
          <p class="float-right">ljubaznost  <progress   max=5 value={{hospitality}}> </progress> {{hospitality}}</p>
          <p class="text-right">venepunkcija  <progress max=5 value="{{venipuncture}}"></progress> {{venipuncture}}</p>
          <p class="text-right">izdavanje rezultata  <progress value="{{speed}}" max="5"> </progress> {{speed}}</p>
          <p class="text-right">covid odgovorni <progress value="{{covid}}" max="5"></progress> {{covid}}</p>
          <p class="text-right">opšti utisak <progress value="{{overall}}" max="5"></progress> {{overall}}</p>
        </div>
        <div class="mt-5" id="labFeaturesAdminArea">
          {{!-- {{#if findLab.disability}} --}}
          <p><img src="/images/disability.svg" class="labInfoWindowOsiguranje" title="dostupno invalidima">Dostupno invalidima</p>
          {{!-- {{/if}} --}}
          {{!-- {{#if findLab.private}} --}}
          <p><img src="/images/osiguranje.svg" class="labInfoWindowOsiguranje" title="privatno osiguranje">Privatno osiguranje</p>
          {{!-- {{/if}} --}}
          {{!-- {{#if findLab.accredited}} --}}
          <p><img src="/images/verified.svg" class="labInfoWindowVerified" title="akreditovano">Akreditovana laboratorija</p>
          {{!-- {{/if}} --}}
          {{!-- {{#if findLab.patronage}} --}}
          <p><img src="/images/ambulance_2.svg" class="labInfoWindowOsiguranje" title="radimo patronažu">Radimo patronažu</p>
          {{!-- {{/if}} --}}
        </div>

        <h1 class="text-light mt-5">{{findLab.labName}}</h1>
        <span></span>
        <p class="text-light labDetailsAdresaLab pt-1">{{findLab.address}}</p>
        <p class="text-light labDetailsTelefoniLab pt-1">{{displayPhoneList findLab.phone}}</p>
        <p class="text-light webLab pt-1">{{findLab.web}}</p>

        
        <table class="table mt-4 table-sm workingHoursLabDetails">
          <thead>
            <tr>
              <th class="text-center text-light">P</th>
              <th class="text-center text-light">U</th>
              <th class="text-center text-light">S</th>
              <th class="text-center text-light">Č</th>
              <th class="text-center text-light">P</th>
              <th class="text-center text-light">S</th>
              <th class="text-center text-light">N</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td class="text-center text-light {{isItOpen status 1}}">{{findLab.workingHours.monday.opens}} - {{findLab.workingHours.monday.closes}}</td>
              <td class="text-center text-light {{isItOpen status 2}}">{{findLab.workingHours.tuesday.opens}} - {{findLab.workingHours.tuesday.closes}}</td>
              <td class="text-center text-light {{isItOpen status 3}}">{{findLab.workingHours.wednesday.opens}} - {{findLab.workingHours.wednesday.closes}}</td>
              <td class="text-center text-light {{isItOpen status 4}}"> {{findLab.workingHours.thursday.opens}} - {{findLab.workingHours.thursday.closes}}</td>
              <td class="text-center text-light {{isItOpen status 5}}"> {{findLab.workingHours.friday.opens}} - {{findLab.workingHours.friday.closes}}</td>
              <td class="text-center text-light {{isItOpen status 6}}">{{findLab.workingHours.saturday.opens}} - {{findLab.workingHours.saturday.closes}}</td>
              <td class="text-center text-light {{isItOpen status 0}}">{{findLab.workingHours.sunday.opens}} - {{findLab.workingHours.sunday.closes}}</td>
            </tr>
          </tbody>
        </table>
        <!-- {{findLab.labName}} -->

          <form>
            <div class="row d-flex align-items-end">
              <div class="col-sm-12 col-lg-8 mx-auto ">
                <input type="text" class="form-control searchForUser z" name="innerSearch" id="searchForUserEmail" placeholder="Unesite email pacijenta">
              </div> <!-- searchField -->
            </div><!-- row -->
          </form><!-- form end -->

      </div>
    </div>
  </div>
</header>
