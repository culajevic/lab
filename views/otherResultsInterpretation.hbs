{{> _innerNavTumacenje}}
<!-- ovo je stranica koju koristi labcube da pogleda ko je sve uradio upload rezultata i da pocne tumacenje -->


<div class="container-fluid w-75">
  <div class="row">
    <!-- <div class=""> -->
      <div class="table-responsive mt-4" id="labDashboard">
        <table class="table tableLabResults" id="analysisResultsCube">
          <tbody>
          {{#each otherResultsForInterpretation}}
            <tr class="dashboardResults">
              <td><span class="{{checkPayment this.package this.paid}}"></span></td>
              <td>{{this.userId.email}}</td>
              <td>{{formatRsDate this.submitedDate}}</td>
              <!-- <td>{{formatRsDate this.deadline}}</td> -->
              <!-- <td>{{this.paid}} rsd</td> -->
              <!-- <td>{{{checkInterpreatationStatus this.status}}}</td> -->
              <td><a href="/results/{{this.result}}" target="_blank"><i class="far fa-file-alt" title="pogledaj fajl"></i></td>
              <td><a class="" href="/otherResultsInterpretation/{{this._id}}">{{#unless (checkStatus this.status)}}<button class="btn btn-block btn-danger text-uppercase">protumači</button>{{else}}<button class="btn btn-block Završeno">Završeno</button>{{/unless}}</a></td>
              <td id="deadline{{this.id}}" class="deadline d-none">{{this.deadline}}</td>
              <!-- <td id="days{{this.id}}" class="days"></td> -->

          {{#if (checkStatus this.status)}}
          <td class="hours"><span class="clappingHands">&#128079;</span></td>
          <td class="mins"><span class="clappingHands">&#128079;</span></td>
          <td class="secs"><span class="clappingHands">&#128079;</span></td>
          {{else}}
              <td id="hours{{this.id}}" class="hours"></td>
              <td id="mins{{this.id}}" class="mins"></td>
              <td id="secs{{this.id}}" class="secs"></td>
            {{/if}}

              {{#if (undefined this.completedDate)}}
                <td>{{formatRsDate this.completedDate}}</td>
                {{else}}
                <td></td>
                {{/if}}
              <td>{{this.owner.username}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
        {{> _paginationCube}}
      </div>
    <!-- </div> -->
  </div>
</div>
