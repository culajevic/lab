{{> _innerNavBackend}}

<div class="container">
  <div class="row">
    {{#if number}}
      <table class="table table-hover">
        <thead>
          <tr>
            <!-- <th scope="col">id</th> -->
            <th scope="col">Naziv</th>
            <th scope="col">Grupa</th>
            <th scope="col">Opis</th>
            <th scope="col">Uredjuje</th>
          </tr>
        </thead>
        <tbody>
          {{#each allAnalysis}}
          <tr>
            <td>{{this.analysisName}}</td>
            <td>{{this.groupName}}</td>
            <td>{{this.preview}}</td>
            <td>{{this.writtenByfirstName}} {{this.writtenBylastName}}</td>
            <td><a href='addAnalysis/{{this._id}}' class="btn btn-info">Edit</a></td>
            <td><a href='' data-id={{this._id}} class="btn btn-danger deleteDocument">Delete</a></td

          </tr>
          {{/each}}
        </tbody>
      </table>
      {{else}}
        <div class="m-auto">
          <h3 class="mt-4">Trenutno nema nijedna analiza u bazi</h3>
        </div>
      {{/if}}
  </div>
</div>

{{> _footer}}
