{{> _innerNavBackend}}
<div class="container-fluid">
  <div class="row">
    <div class="col-12 px-5">
        <table class="table table-hover mt-5">
          <thead>
            <tr>
              <!-- <th scope="col">id</th> -->
              <th scope="col">Ime</th>
              <th scope="col">Email</th>
              <th scope="col">Datum</th>
              <th scope="col">Verifikovan</th>
              <th scope="col">Grad</th>
              <th scope="col">News</th>
              <th scope="col">Poslednji login</th>

            </tr>
          </thead>
          <tbody>
            {{#each allUsers}}
            <tr>
              <td>{{this.username}}</td>
              <td>{{this.email}}</td>
              <td>{{formatRsDate this.signupDate}}</td>
              <td>{{this.isVerified}}</td>
              <td>{{this.city}}</td>
              <td>{{this.news}}</td>
              <td>{{formatRsDate lastLoginDate}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>

    </div>
  </div>
</div>
